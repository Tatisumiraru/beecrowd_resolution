def funcion():
    n =3.14159
    raio = 2.00 **2
    saida = n * raio
    print(f'A={saida:.4f}')

def funcion_2():
    n =3.14159
    raio = 100.64 **2
    saida = n * raio
    print(f'A={saida:.4f}')

def funcion_3():
    n =3.14159
    raio = 150.00 **2
    saida = n * raio
    print(f'A={saida:.4f}')

funcion()
funcion_2()
funcion_3()
